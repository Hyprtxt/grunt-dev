// footer.styl

.prefooter
	background #eee
	text-align center
	padding 50px
	h2
		width 700px
		margin 0 auto
		padding-bottom 30px

.footer
	background #646263
	padding 20px 30px
	ul
		width 800px
		list-style none
		margin 0 auto
		padding-bottom 30px
	li
		float left
		border-right 1px solid white
		line-height 12px
	li:last-of-type
		border none
	a
		padding 0 15px
	p
		clear both
		text-align center
	a, p
		color white
		font-family sans-serif
		text-transform uppercase
		font-size 11px